body
{
	font-family: Arial;
	font-weight: normal;
	font-style: normal;
	font-size: 1em;
	font-charset: U+0020-007E,U+00A9; /* This controls which unicode characters can be used in text */
	background-color: rgba(0%, 0%, 0%, 65%);
	width: 100%;
	height: 100%;
	color: #7b7b7b;
}

div#content
{
	margin: auto;
	margin-top: 15%;
	width: 50%;
	height: auto;
	overflow: hidden;
	position: relative;
}
